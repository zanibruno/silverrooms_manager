<h1>I am new_tenant_path </h1>
<% if @tenant.errors.any? %>
<div id="error_explanation">
    <ul>
        <% @tenant.errors.full_messages.each do |msg| %>
        <li> <%= msg %> </li>
        <% end %>
    </ul>
</div>
<% end %>

<%=form_for @tenant do |f| %>
<%=f.label "Tenant Name " %>
<%=f.text_field :name %></br>
<%=f.label "Phone Number" %>
<%=f.text_field :phone_number %></br>
<%=f.label "Email" %>
<%=f.text_field :email %></br>
<%=f.label "Emergency Contatc Name" %>
<%=f.text_field :sos_name %></br>
<%=f.label "Emergy Contact Number" %>
<%=f.text_field :sos_number %></br>
<%=f.label "Property" %>
<%=f.collection_select(:property_id, Property.all, :id, :address)%></br>
<%=f.label "Start Date" %>
<%=f.date_field :start_date %></br>
<%=f.label "End Date" %>
<%=f.date_field :end_date %></br>
<%=f.submit "Create Tenant" %>
<%end  %>
